
@{
    ViewBag.Title = "DisplayShippedDrugs";
}

<h2>DisplayShippedDrugs</h2>

<table class="table-bordered text-center">
    <thead>
        <tr>
            <th><p>Drug unit id</p></th>
        </tr>
    </thead>

    @foreach (var d in ViewBag.DrugUnitsId)
    {
        <tr>
            <td><p>   @d</p></td>
        </tr>
    }
</table>            